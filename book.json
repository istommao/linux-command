{
    "title": "Linux Command Notes",
    "description": "Linux命令笔记",
    "language": "zh-hans",
    "plugins": [
        "github",
        "prism",
        "sitemap",
        "toc2",
        "expandable-chapters",
        "page-toc-button",
        "back-to-top-button"
    ],
    "pluginsConfig": {
        "github": {
          "url": "https://github.com/istommao/linux-command"
        },
        "sitemap": {
            "hostname": "https://os.silentnotes.top/"
        },
        "prism": {
            "lang": {
                "console": "bash",
                "shell": "bash",
                "clang": "c"
            },
            "css": [
              "prismjs/themes/prism-okaidia.css"
            ]
        },
        "toc2": {
            "addClass": true,
            "className": "toc"
        },
        "expandable-chapters":{}
    }
}